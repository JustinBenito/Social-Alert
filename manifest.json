{
    "manifest_version": 2,
    "name": "Social Alert",
    "version": "1.0",
    "description": "Alerts your parents if you enter Social Media",
    "permissions": [
      "activeTab"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Social Alert",
      "default_icon": {                
      "16": "./icons.png"
    }
    }
  }
  